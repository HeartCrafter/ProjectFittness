<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.psu.ist402.projectfittness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserEntryActivity"
            android:label="@string/title_activity_entry_activity"></activity>
        <activity
            android:name=".UserSummaryActivity"
            android:label="@string/title_activity_user_summary"></activity>
        <activity
            android:name=".ExerciseActivity"
            android:label="@string/title_activity_workout"></activity>
        <activity
            android:name=".HelpActivity"
            android:label="@string/title_activity_help"></activity>
    </application>

</manifest>
